{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "alvin0/database-json-laravel": "^2.1",
        "appstract/laravel-options": "^2.2",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "bensampo/laravel-enum": "^1.0",
        "chriskonnertz/string-calc": "^1.0",
        "coderello/laravel-shared-data": "^3.2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "hisorange/browser-detect": "^4.5",
        "izniburak/laravel-auto-routes": "^1.0",
        "joedixon/laravel-translation": "^1.2",
        "kirschbaum-development/eloquent-power-joins": "^2.3",
        "kyslik/column-sortable": "^6.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "laravolt/avatar": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mehradsadeghi/laravel-filter-querystring": "^1.1",
        "milon/barcode": "^6.0",
        "msztorc/laravel-env": "^1.2",
        "spatie/laravel-backup": "^6.16",
        "touhidurabir/laravel-model-sanitize": "^1.1",
        "wildside/userstamps": "^2.2"
    },
    "require-dev": {
        "orangehill/iseed": "^3.0",
        "pelmered/fake-car": "^1.7",
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-query-detector": "^1.6",
        "dietercoopman/laravel-showsql": "^1.3",
        "filp/whoops": "^2.0",
        "kitloong/laravel-migrations-generator": "^6.3",
        "laravel/ui": "1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Plugins\\": "Plugins/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
